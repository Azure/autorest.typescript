{
  "name": "@msinternal/anomaly-detector-mv-rest",
  "version": "1.0.0-beta.1",
  "description": "A generated SDK for AnomalyDetectorMV.",
  "keywords": [
    "node",
    "azure",
    "cloud",
    "typescript",
    "browser",
    "isomorphic"
  ],
  "license": "MIT",
  "author": "Microsoft Corporation",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "./dist-esm/index.js",
  "types": "./types/anomaly-detector-mv-rest.d.ts",
  "files": [
    "dist/",
    "dist-esm/",
    "types/anomaly-detector-mv-rest.d.ts",
    "README.md",
    "LICENSE",
    "review/*"
  ],
  "scripts": {
    "build": "npm run clean && tsc && rollup -c 2>&1 && npm run minify && mkdirp ./review && npm run extract-api",
    "check-format": "prettier --list-different --config ../../../.prettierrc.json --ignore-path ../../../.prettierignore \"src/**/*.ts\" \"*.{js,json}\"  \"test/**/*.ts\" \"samples-dev/**/*.ts\"",
    "clean": "rimraf --glob dist dist-browser dist-esm test-dist temp types *.tgz *.log",
    "extract-api": "rimraf review && mkdirp ./review && api-extractor run --local",
    "format": "prettier --write --config ../../../.prettierrc.json --ignore-path ../../../.prettierignore \"src/**/*.ts\" \"*.{js,json}\"  \"test/**/*.ts\" \"samples-dev/**/*.ts\"",
    "lint": "eslint package.json api-extractor.json src  --ext .ts",
    "lint:fix": "eslint package.json api-extractor.json src  --ext .ts --fix --fix-type [problem,suggestion]",
    "minify": "uglifyjs -c -m --comments --source-map \"content='./dist/index.js.map'\" -o ./dist/index.min.js ./dist/index.js",
    "pack": "npm pack 2>&1"
  },
  "dependencies": {
    "@azure-rest/core-client": "^1.2.0",
    "@azure/core-auth": "^1.6.0",
    "@azure/core-paging": "^1.5.0",
    "@azure/core-rest-pipeline": "^1.5.0",
    "@azure/logger": "^1.0.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.40.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-multi-entry": "^6.0.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/node": "^18.0.0",
    "autorest": "latest",
    "dotenv": "^16.0.0",
    "eslint": "^8.55.0",
    "mkdirp": "^2.1.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^2.66.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "source-map-support": "^0.5.9",
    "typescript": "~5.3.3",
    "uglify-js": "^3.4.9"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "autoPublish": false
}
