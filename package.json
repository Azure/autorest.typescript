{
  "name": "@microsoft.azure/autorest.typescript",
  "version": "2.0.0",
  "description": "The typescript extension for classic generators in AutoRest.",
  "scripts": {
    "autorest": "autorest",
    "start": "dotnet src/bin/netcoreapp2.0/autorest.typescript.dll --server",
    "start-test-server": "ts-node -T testserver",
    "pretest": "npm install",
    "test": "gulp test",
    "testci": "gulp testci",
    "testci/generator-unit": "gulp testci/generator-unit",
    "regenerateci": "gulp regenerateci",
    "build": "gulp build",
    "prepack": "npm install && gulp install_common && gulp build",
    "publish-preview": "gulp publish-preview --release",
    "regenerate": "gulp regenerate",
    "clean": "gulp clean",
    "nuke": "git clean -xdf",
    "local": "ts-node -T ./.scripts/local.ts",
    "preview": "ts-node -T ./.scripts/preview.ts",
    "latest": "ts-node -T ./.scripts/latest.ts",
    "bundle-size": "ts-node -T ./.scripts/bundlesize.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Azure/autorest.typescript"
  },
  "readme": "https://github.com/Azure/autorest.typescript/readme.md",
  "keywords": [
    "autorest",
    "classic-generators",
    "typescript"
  ],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Azure/autorest/issues"
  },
  "homepage": "https://github.com/Azure/autorest.typescript/blob/master/README.md",
  "devDependencies": {
    "@azure/ms-rest-azure-js": "^1.1.0",
    "@azure/ms-rest-js": "^1.1.0",
    "@microsoft.azure/autorest.modeler": "^2.3.51",
    "@microsoft.azure/autorest.testserver": "2.6.0",
    "@types/assert": "0.0.31",
    "@types/express": "^4.11.1",
    "@types/filesize": "^3.6.0",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.0",
    "@types/glob": "^5.0.35",
    "@types/mocha": "^2.2.48",
    "@types/node": "^10.11.3",
    "@types/should": "8.1.30",
    "@types/webpack": "^4.4.5",
    "@types/webpack-dev-server": "^2.9.5",
    "abortcontroller-polyfill": "^1.1.9",
    "arraybuffer-loader": "^1.0.4",
    "autorest": "^2.0.4283",
    "coffee-script": "^1.11.1",
    "dotnet-sdk-2.0.0": "^1.4.4",
    "express": "^4.16.3",
    "filesize": "^3.6.1",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-filter": "^5.0.0",
    "gulp-line-ending-corrector": "^1.0.1",
    "iced-coffee-script": "^108.0.11",
    "marked": "^0.3.6",
    "marked-terminal": "^2.0.0",
    "mocha": "^5.2.0",
    "mocha-chrome": "^1.0.3",
    "moment": "~2.21.0",
    "run-sequence": "*",
    "should": "5.2.0",
    "shx": "^0.2.2",
    "through2-parallel": "^0.1.3",
    "ts-loader": "^4.1.0",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "tslint": "^5.10.0",
    "typescript": "^3.1.6",
    "underscore": "^1.8.3",
    "webpack": "~4.19.1",
    "webpack-cli": "~2.1.5",
