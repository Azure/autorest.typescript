    "dotenv": "^8.2.0",
    "eslint": "^8.0.0",
    "mkdirp": "^1.0.4",
    "prettier": "2.2.1",
    "rimraf": "^3.0.0",
    "source-map-support": "^0.5.9",
    "typescript": "~4.6.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-multi-entry": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "rollup": "^2.66.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "uglify-js": "^3.4.9"
