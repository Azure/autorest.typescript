    {{#unless isTopLevel}}
    .{{operationGroupName}}
    {{/unless}}
    .{{operationName}}({{this.methodParameterNames}});
  console.log(result);
  
  {{/if}}
}

{{this.sampleFunctionName}}().catch(console.error);
